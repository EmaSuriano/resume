{
  "name": "astro-resume",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "generate-pdf": "ts-node --project tsconfig-pdf.json ./scripts/generate-pdf.ts",
    "generate-pdf:ci": "start-server-and-test preview 3000 generate-pdf",
    "build": "astro check && astro build && yarn generate-pdf:ci",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/tailwind": "^2.1.1",
    "astro": "^1.5.2",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@fontsource/poppins": "^4.5.10",
    "@fontsource/twinkle-star": "^4.5.9",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^18.11.6",
    "playwright": "^1.27.1",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1"
  }
}
